@model System.Web.Mvc.HandleErrorInfo

@{
    ViewBag.Title = "Error";
}

<div class="container">
    <h1 class="text-danger">Error.</h1>
    <h2 class="text-danger">@ViewBag.ErMes</h2>
    <h3 class="text-danger">@ViewBag.ErInner</h3>
    <h4 class="text-danger">@ViewBag.ErStack</h4>

</div>
